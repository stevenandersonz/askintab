{
  "name": "Your Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "debugger"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://chatgpt.com/*" 
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
